        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <search>
          <query>index="ynab" NOT Payee="Transfer*" | eval sum_out=round(Outflow,0) | eval sum_in=round(Inflow,0) | reverse | accum sum_out as Spendings | accum sum_in as Income | timechart last(Income) as Income, last(Spendings) as Spendings span=1week</query>
